    "babel-loader": "^8.0.6",
    "babel-runtime": "^6.20.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^2.1.1",
    "enzyme": "^2.7.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "ignore-styles": "^5.0.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.3.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^4.8.6",
    "redux-freeze": "^0.1.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
